@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Home>
@using ContentModels = Umbraco.Web.PublishedModels;

<section id="about" class="about">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <img src="assets/img/about.png" class="img-fluid" alt="">
            </div>
            <div class="col-lg-6 pt-4 pt-lg-0 content">
                <h3>Takerman</h3>
                <p class="font-italic">
                    We are just another software development company.
                </p>
                <ul>
                    <li><i class="icofont-check-circled"></i> .NET</li>
                    <li><i class="icofont-check-circled"></i> Umbraco</li>
                    <li><i class="icofont-check-circled"></i> Bootstrap</li>
                </ul>
                <p>
                    Many technologies
                </p>
            </div>
        </div>
    </div>
</section>