        <h1 class="logo mr-auto"><a href="/">@Model.SiteName</a></h1>
        @*<a href="index.html" class="logo mr-auto"><img src="assets/img/logo.png" alt="" class="img-fluid"></a>*@
        <nav class="nav-menu d-none d-lg-block">
            <ul>
                <li class="active"><a href="#header">@Model.NavHome</a></li>
                <li><a href="#about">@Model.NavAbout</a></li>
                @*<li><a href="#services">Services</a></li>*@
                @*<li><a href="#portfolio">Portfolio</a></li>*@
                <li><a href="#team">@Model.NavTeam</a></li>
                <li><a href="#contact">@Model.NavContact</a></li>
            </ul>
        </nav>
        <a href="#about" class="get-started-btn scrollto">@Model.GetStarted</a>
    </div>
</header>
